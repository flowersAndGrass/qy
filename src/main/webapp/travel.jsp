<script>
var useruid = '9948068';
var type = 'userhometravellist';
var filter = '';
var htmlReply = '<div class="home-reply-wrap"><p class="hri-p">我在<a class="hri-tle" href="#turl#" target="_blank">#title#</a>中回复<span style="display:#born#"><a class="hri-name" href="#userurl#" target="_blank">@#replay_username#</a>的评论</span></p><p class="hri-qte" style="display:#qtedn#">#quote#</p><p class="hri-content">#content#</p><p><span class="hri-time">#time#</span><span class="hri-a"><a href="javascript:;" class="hri-del" data-id="#pid#">删除</a><a href="#posturl#" target="_blank" class="hri-lok">查看</a></span></p></div>';
function userhomethreadlistpage(page,offsetstatus){
    if(parseInt(page) == NaN || page < 1){
        page = 1;
    }

    var apiurl, postdata;

    switch (type) {

        case 'userhomepostslist':
            apiurl = '/ajax.php';
            postdata = {action:"getuserhomethreadlist",page:page,useruid:useruid,type:type,filter:filter};
            break;

        case 'userhomedraftlist':
            apiurl = '/qcross/bbs/draft.php?action=list&_dataFormat=ucenter';
            postdata = {};
            break;
        case 'userhomereply':
            apiurl = '/qcross/bbs/post.php?action=myList&page='+page;
            break;

        default:
            apiurl = '/ajax.php';
            postdata = {action:"getuserhomethreadlist",page:page,useruid:useruid,type:type};
    }

    jQuery.post(apiurl,postdata,function(html){
        if (html) {
            if(type === "userhomereply"){
                var strs = "<div class='user-home-reply'>"; 
                jQuery("#userhomethreadlist").html("");
                var ht = $.parseJSON(html);
                if(ht.result === "ok" && ht.data.list && ht.data.list.length){
                    $(".myReplyNum").html(ht.data.total);
                    $.each(ht.data.list,function(idx,item){
                        strs = strs + htmlReply.replace(/#title#/,item.subject)
                        .replace(/#turl#/,item.threadurl).replace(/#born#/,item.replay_username ? "inline":"none")
                        .replace(/#userurl#/,item.userurl).replace(/#replay_username#/,item.replay_username)
                        .replace(/#qtedn#/,item.quote ? "display":"none").replace(/#posturl#/,item.posturl)
                        .replace(/#pid#/,item.id).replace(/#quote#/,item.quote)
                        .replace(/#content#/,item.content)
                        .replace(/#time#/,item.create);
                    });
                
                    jQuery("#userhomethreadlist").html(strs + ht.data.pageHtml + "</div>");
                    $.each($(".hri-qte"),function(idx,item){
                        countShowLen($(item),40);
                    });
                    $.each($(".hri-content"),function(idx,item){
                        countShowLen($(item),70);
                    });
                }
                
            
            }else{
                jQuery("#userhomethreadlist").html(html);
                    if(typeof offsetstatus == 'undefined'){
                        $("html,body").animate(
                            {
                            scrollTop:$(".u_travel_main").offset().top
                            },200
                    );
                }
        
            }
        }
    });
}
var countShowLen = function(dom,eliHeight){
    if(dom.height() > eliHeight){
        !+dom.attr("data-iscount") && dom.text(dom.text() + "...");
        dom.attr("data-iscount",1);
        var text = dom.text();
        dom.text(text.substring(0,text.length - 4)+"...");
        countShowLen(dom,eliHeight);
    }
};
jQuery(function($){
    $(".u_travel_main .tag a").on("click",function(){
        type = $(this).attr("data-type");
        $(".u_travel_main .tag a").removeClass("current");
        $(this).addClass("current");
        userhomethreadlistpage(1,1);
    })

    $("#userhomethreadlist").on("click",".tag_list a,.hri-del",function(){
        var me = $(this);
        if(me.hasClass("hri-del")){
            me.parents(".home-reply-wrap").slideUp(); 
            $(".myReplyNum").html(+$(".myReplyNum").html() - 1);
            $.post("/qcross/bbs/post.php?action=remove",{pid:me.attr("data-id")},function(suc){
            }); 
        }else{
            filter = $(this).attr("data-filter");
            $("#userhomethreadlist .tag_list a").removeClass("current");
            $(this).addClass("current");
            userhomethreadlistpage(1,1);
        }
    });
    var www_m = "collect";
    var exp_m = new RegExp(www_m, "i");
    // 加载收藏帖子
    if(exp_m.test(window.location.href)){//不 m.qyer.com 不显示
        type = "userhomefavoritesslist";
        $(".u_travel_main .tag a").removeClass("current");
        $(".u_travel_main .tag a[data-type="+type+"]").addClass("current");
        userhomethreadlistpage(1,1);
    }
    // 加载其他帖子
    if(/other/i.test(window.location.href)){
        type = "userhomeotherthreadlist";
        $(".u_travel_main .tag a").removeClass("current");
        $(".u_travel_main .tag a[data-type="+type+"]").addClass("current");
        userhomethreadlistpage(1,1);
    }
    // 加载我的草稿
    if(/draft/i.test(window.location.href)){
        type = "userhomedraftlist";
        $(".u_travel_main .tag a").removeClass("current");
        $(".u_travel_main .tag a[data-type="+type+"]").addClass("current");
        userhomethreadlistpage(1,1);
