	<script type="text/javascript">
		function initjcrop() {

			// Create variables (in this scope) to hold the API and image size
			var jcrop_api,
				boundx,
				boundy,

				// Grab some information about the preview pane
				$preview = $('#preview-pane'),
				$pcnt = $('#preview-pane .preview-container'),
				$pimg = $('#preview-pane .preview-container img'),

				xsize = $pcnt.width(),
				ysize = $pcnt.height();

			console.log($('#target')[0].src);
			$('#target').Jcrop({
				boxWidth: 370, //
				boxHeight: 700,
				onChange: updatePreview,
				onSelect: updatePreview,
				//setSelect: [ xsize/2, ysize/2, 200, 200 ],
				aspectRatio: xsize / ysize
			}, function() {
				// Use the API to get the real image size
				var bounds = this.getBounds();
				boundx = bounds[0];
				boundy = bounds[1];
				// Store the API in the jcrop_api variable
				jcrop_api = this;
				var pic_curw = $(".jcrop-holder").find("img").width();
				var pic_curh = $(".jcrop-holder").find("img").height();
				jcrop_api.animateTo([pic_curw / 2 - 100, pic_curh / 2 - 100, pic_curw / 2 + 100, pic_curh / 2 + 100]);
				//alert(pic_curw);
				/*$("#x").val(pic_curw/2 - 100);
				$("#y").val(pic_curh/2 - 100);
				$("#w").val(pic_curw/2 + 100);
				$("#h").val(pic_curh/2 + 100);
				$("#w1").val(pic_curw);
				$("#h1").val(pic_curh);*/
				// Move the preview into the jcrop container for css positioning
				$preview.appendTo(jcrop_api.ui.holder);
			});

			function updatePreview(c) {
				if(parseInt(c.w) > 0) {
					var rx = xsize / c.w;
					var ry = ysize / c.h;
					$('#x').val(c.x);
					$('#y').val(c.y);
					$('#w').val(c.w);
					$('#h').val(c.h);
					$pimg.css({
						width: Math.round(rx * boundx) + 'px',
						height: Math.round(ry * boundy) + 'px',
						marginLeft: '-' + Math.round(rx * c.x) + 'px',
						marginTop: '-' + Math.round(ry * c.y) + 'px'
					});
				}
			};
		}

		$(document).ready(function() {
