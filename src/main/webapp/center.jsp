        <script>
        $(function(){
            //talk留言模块
            var talks = $("#mod_talks");

            //文本域focus事件
            talks.find(".ui2_textarea").focus(function(){
                $(this).addClass("ui2_focus");
                $(this).removeClass("ui2_error");
                $(this).parents("form").find(".ui2_error_layer").hide();
            });

            //文本域blur事件
            talks.find(".ui2_textarea").blur(function(){
                $(this).removeClass("ui2_focus");
            });

            //文本域字数统计
            talks.find(".ui2_textarea").keyup(function(){
                var size = $(this).val().length;
                var sum = $(this).parents("form").find(".sum");
                //console.log(size);
                sum.text(size);
                if(size > 30){
                    sum.addClass("sum_error");
                }
                else {
                    sum.removeClass("sum_error");
                }
            });

            //item列表hover事件
            talks.find(".mod_talk_list_item").hover(function(){
                $(this).find(".handle").show();
            },function(){
                $(this).find(".handle").hide();
            });

            //reply回复 - 点击显示隐藏
