/* 相册大小定位 */
$(function(){
    var photoBox = $(".u_ind_photo");
    var photoLayer = photoBox.find(".photo_list");
    var photoImg = photoLayer.find(".img img");
    photoImg.load(function(){
        var thiz = $(this);
        var thizWidth = thiz.width();
        var thizHeight = thiz.height();
        var layerWidth = thiz.parent().width();
        var layerHeight = thiz.parent().height();

        var endWidth = layerWidth - thizWidth;
        var endHeight = layerHeight - thizHeight;

        if(endWidth > endHeight){
            thiz.css("width","100%");
            var thizResetHeight = thiz.height();
            var endTop = (thizResetHeight - layerHeight) / 2;
            thiz.css("top",-endTop);
        }

        if(endWidth < endHeight){
            thiz.css("height","100%");
            var thizResetWidth = thiz.width();
            var endLeft = (thizResetWidth - layerWidth) / 2;
            thiz.css("left",-endLeft);
        }

    });
});
$(function(){
    //头部大图
    (function(){
        var img = $('#u_ind_banner .img img');
        var nav = $('.q-layer-header');
        var valve = 340;

        $(window).scroll(function(){
            var scrTop = $(window).scrollTop();
            if (scrTop<=valve) {
                nav.css('top', 0);
                img.css('top', scrTop/2 );
            }
            else {
                nav.css('top', -50);
            }
        });
        if ( /msg/.test(window.location) ) {
            return false;
        }
        if (window.location.hash == "") {
            setTimeout(function(){
                var top = 300;
                $("html,body").animate({scrollTop:top});
            }, 1500);
        }
    })();


    //来访用户
    (function(){
        var list = $("#visitor_list");
        var items = list.find("li");
        var prev = $("#visitor_prev");
        var next = $("#visitor_next");
        var size = items.size(); //item数量
        var scn = size / 7;
        scn = Math.ceil(scn); //可滚几屏
        var scn_width = 270; //单个宽度
        var sum_width = 40 * size; //总宽度
        var index = 0; //索引

        if(scn > 1){
            list.width(sum_width);
            prev.click(function(){
                //console.log(index);
                if(index > 0){
                    index --;
                    //console.log(index);
                    next.removeClass("null");
                    list.animate({left:-scn_width*index});
                }
                if(index == 0){
                    $(this).addClass("null");
                }
            });
            next.click(function(){
                //console.log(index);
                if(index < scn - 1){
                    index ++;
                    //console.log(index);
                    prev.removeClass("null");
                    list.animate({left:-scn_width*index});
                }
                if(index == scn - 1){
                    $(this).addClass("null");
                }
            });
        }
    })();
});
</script>
